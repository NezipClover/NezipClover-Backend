<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ssafy.Whereismyhouse.house.model.dao.HouseDealDao">
	
<select id="search" parameterType="String" resultType="HouseDeal">
select * from housedeal
where aptCode = #{aptCode}
</select>


<select id="EntireDealAmount" resultType="EntireDealAmount">
select dongName, count(dongName) as cnt from housedeal as hd
left join houseinfo as hi
on hd.aptCode = hi.aptCode
group by dongName
order by cnt desc
limit 0, 11
</select>


	
</mapper>