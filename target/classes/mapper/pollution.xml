<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ssafy.Whereismyhouse.house.model.dao.PollutionDao">
	
<select id="searchByDong" parameterType="map" resultType="Pollution">
select *               
from pollution         
where dongCode =#{dongCode}      
and dong=#{dongName}

</select>

	
</mapper>